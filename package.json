{
  "name": "createjs-drawable-bitmap",
  "version": "0.1.0",
  "main": "./bin/index.js",
  "types": "./bin/index.d.ts",
  "repository": "https://MasatoMakino@github.com/MasatoMakino/createjs-drawable-bitmap.git",
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@masatomakino/gulptask-demo-page": "^0.6.7",
    "@types/easeljs": "^1.0.1",
    "browser-sync": "^2.29.3",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run buildTS && npm run demo && npm run typedocs",
    "buildTS": "tsc --project tsconfig.json",
    "watch:tsc": "tsc -W --incremental --project tsconfig.json",
    "demo": "npx @masatomakino/gulptask-demo-page --externalScripts 'https://code.createjs.com/1.0.0/easeljs.min.js' ",
    "watch:demo": "npm run demo -- -W",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "npx browser-sync ./docs/demo -w",
    "prepare": "husky install"
  }
}
